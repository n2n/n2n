{
	"name" : "n2n/n2n",
	"type" : "n2n-module",
	"description" : "Combines diffrent components of n2n framework.",
	"keywords" : [
		"n2n",
		"framework"
	],
	"homepage" : "http://www.n2n.ch",
	"license" : "LGPL-3.0-or-later",
	"authors" : [{
			"name" : "Andreas von Burg",
			"email" : "a@von-burg.net",
			"homepage" : "https://andreas.von-burg.net"
		}, {
			"name" : "Bert Hofmänner",
			"email" : "hofmaenner@hnm.ch",
			"homepage" : "http://www.hnm.ch"
		}, {
			"name" : "Thomas Günther",
			"email" : "guenther@hnm.ch"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"config" : {
		"vendor-dir" : "src/vendor"
	},
	"require" : {
		"php" : ">=7.0",
		"n2n/n2n-composer-module-installer" : "^7.1",
		"n2n/n2n-config" : "^7.3",
		"n2n/n2n-l10n" : "^7.3",
		"n2n/n2n-log4php" : "^7.2.33",
		"n2n/n2n-mail" : "^7.2.33",
		"n2n/n2n-reflection" : "^7.3",
		"n2n/n2n-util" : "^7.3"
	},
	"autoload" : {
		"psr-4" : {
			"n2n\\core\\" : "src/app/n2n/core"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"n2n\\core\\" : "src/test/n2n/core"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.5"
	}
}