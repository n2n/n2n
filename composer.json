{
	"name" : "n2n/n2n",
	"type" : "n2n-module",
	"description" : "Combines different components of n2n framework.",
	"keywords" : [
		"n2n",
		"framework"
	],
	"homepage" : "https://www.n2n.ch",
	"license" : "LGPL-3.0-or-later",
	"authors" : [
		{
			"name" : "Andreas von Burg",
			"email" : "a@von-burg.net",
			"homepage" : "https://andreas.von-burg.net"
		},
		{
			"name" : "Bert Hofmänner",
			"email" : "hofmaenner@hnm.ch",
			"homepage" : "https://www.hnm.ch"
		},
		{
			"name" : "Thomas Günther",
			"email" : "guenther@hnm.ch"
    	},
		{
			"name" : "David Meier",
      		"email" : "meier@hnm.ch"
        }
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"config" : {
		"vendor-dir" : "src/vendor",
		"allow-plugins": {
			"n2n/n2n-composer-module-installer": true
		}
	},
	"require" : {
		"n2n/n2n-composer-module-installer" : "^7.1.5",
        "n2n/n2n-spec-tx": "^1.0",
        "n2n/n2n-config" : "~7.4",
        "n2n/n2n-context" : "~7.5",
		"n2n/n2n-l10n" : "~7.4",
		"n2n/n2n-log4php" : "~7.4",
		"n2n/n2n-reflection" : "~7.4",
		"n2n/n2n-util": "~7.4.3",
        "ext-mbstring": "*"
    },
	"autoload" : {
		"psr-4" : {
			"n2n\\core\\" : "src/app/n2n/core"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"n2n\\core\\" : "src/test/n2n/core"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.6.19"
	}
}
